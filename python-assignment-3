number = input("Please enter a number: ")
list = [2,3,4,5,6,7,8,9]
count = 0
for i in list:
  if not int(number)%int(i) == 0:
    count +=1
    if count == 8:
      print("Number is a prime number")
  else : 
    print("Number is not a prime number")
    break

  
  

